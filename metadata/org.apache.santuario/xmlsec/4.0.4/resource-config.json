{
  "resources": {
    "includes": [
      {
        "condition": {
          "typeReachable": "org.apache.xml.security.stax.ext.XMLSecurityConstants"
        },
        "pattern": "\\QMETA-INF/services/javax.xml.datatype.DatatypeFactory\\E"
      },
      {
        "condition": {
          "typeReachable": "org.apache.xml.security.parser.XMLParserImpl"
        },
        "pattern": "\\QMETA-INF/services/javax.xml.parsers.DocumentBuilderFactory\\E"
      },
      {
        "condition": {
          "typeReachable": "org.apache.xml.security.stax.ext.XMLSecurityConstants"
        },
        "pattern": "\\QMETA-INF/services/javax.xml.stream.XMLOutputFactory\\E"
      },
      {
        "condition": {
          "typeReachable": "org.apache.xml.security.encryption.XMLCipher"
        },
        "pattern": "\\QMETA-INF/services/javax.xml.transform.TransformerFactory\\E"
      }
    ]
  },
  "bundles": [
    {
      "name": "org/apache/xml/security/resource/xmlsecurity",
      "locales": [
        "en"
      ]
    }
  ]
}
