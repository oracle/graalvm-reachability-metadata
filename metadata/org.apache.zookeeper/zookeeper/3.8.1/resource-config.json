{
  "resources": {
    "includes": [
      {
        "condition": {
          "typeReachable": "org.apache.zookeeper.server.ZooKeeperServer"
        },
        "pattern": "\\QMETA-INF/services/ch.qos.logback.classic.spi.Configurator\\E"
      },
      {
        "condition": {
          "typeReachable": "org.apache.zookeeper.Environment"
        },
        "pattern": "\\QMETA-INF/services/java.net.spi.InetAddressResolverProvider\\E"
      },
      {
        "condition": {
          "typeReachable": "org.apache.zookeeper.server.ZooKeeperServer"
        },
        "pattern": "\\Qlogback-test.xml\\E"
      },
      {
        "condition": {
          "typeReachable": "org.apache.zookeeper.server.ZooKeeperServer"
        },
        "pattern": "\\Qlogback.xml\\E"
      },
      {
        "condition": {
          "typeReachable": "org.apache.zookeeper.server.ZooKeeperServer"
        },
        "pattern": "\\Qorg/slf4j/impl/StaticLoggerBinder.class\\E"
      },
      {
        "condition": {
          "typeReachable": "org.apache.zookeeper.jmx.MBeanRegistry"
        },
        "pattern": "jdk.jfr:\\Qjdk/jfr/internal/query/view.ini\\E"
      }
    ]
  },
  "bundles": []
}
